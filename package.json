{
  "name": "library",
  "version": "2.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run tauri:serve",
    "build": "npm run tauri:bundle",
    "linux:start": "toolbox run bash -c 'source $HOME/.bashrc && npm start'",
    "linux:build": "toolbox run bash -c 'source $HOME/.bashrc && npm run build'",
    "linux:build_debug": "toolbox run bash -c 'source $HOME/.bashrc && npm run build --debug'",
    "web:serve": "ng serve -o",
    "web:build": "ng build --base-href ./",
    "web:dev": "npm run web:build",
    "web:prod": "npm run web:build -- -c production",
    "tauri": "tauri",
    "tauri:serve": "tauri dev",
    "tauri:bundle": "tauri build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.0.0",
    "@angular/cdk": "^15.1.0",
    "@angular/common": "^16.0.0",
    "@angular/compiler": "^16.0.0",
    "@angular/core": "^16.0.0",
    "@angular/forms": "^16.0.0",
    "@angular/material": "^15.1.0",
    "@angular/platform-browser": "^16.0.0",
    "@angular/platform-browser-dynamic": "^16.0.0",
    "@angular/router": "^16.0.0",
    "@tauri-apps/api": "1.2.0",
    "material-design-icons": "^3.0.1",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.0.0",
    "@angular-eslint/builder": "16.0.1",
    "@angular-eslint/eslint-plugin": "16.0.1",
    "@angular-eslint/eslint-plugin-template": "16.0.1",
    "@angular-eslint/schematics": "16.0.1",
    "@angular-eslint/template-parser": "16.0.1",
    "@angular/cli": "~16.0.0",
    "@angular/compiler-cli": "^16.0.0",
    "@angular/localize": "^16.0.0",
    "@tauri-apps/cli": "1.2.2",
    "@types/jasmine": "~4.3.0",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "eslint": "^8.39.0",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "prettier": "2.8.3",
    "typescript": "~5.0.4"
  }
}
