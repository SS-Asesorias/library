{
  "name": "library",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "toolbox run bash -c 'source $HOME/.bashrc && npm run tauri:serve'",
    "build": "toolbox run bash -c 'source $HOME/.bashrc && npm run tauri:bundle'",
    "web:serve": "ng serve -o",
    "web:build": "ng build --base-href ./",
    "web:dev": "npm run web:build",
    "web:prod": "npm run web:build -- -c production",
    "tauri": "tauri",
    "tauri:serve": "tauri dev",
    "tauri:bundle": "tauri build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.0.0",
    "@angular/cdk": "^15.1.0",
    "@angular/common": "^15.0.0",
    "@angular/compiler": "^15.0.0",
    "@angular/core": "^15.0.0",
    "@angular/forms": "^15.0.0",
    "@angular/material": "^15.1.0",
    "@angular/platform-browser": "^15.0.0",
    "@angular/platform-browser-dynamic": "^15.0.0",
    "@angular/router": "^15.0.0",
    "@tauri-apps/api": "1.2.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.5",
    "@angular/cli": "~15.0.5",
    "@angular/compiler-cli": "^15.0.0",
    "@angular/localize": "^15.1.0",
    "@tauri-apps/cli": "1.2.2",
    "@types/jasmine": "~4.3.0",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~4.8.2"
  }
}